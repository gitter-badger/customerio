{
    "name": "printu/customerio",
    "description": "PHP API for Customer.io",
    "keywords": ["http", "rest", "customer.io", "customerio", "api", "client"],
    "type": "library",
    "license": "MIT",
    "homepage": "http://customer.io/",
    "authors": [
        {
            "name": "PaweÅ‚ Krzaczkowski",
            "email": "pawel@freshmind.pl",
            "homepage": "https://github.com/krzaczek"
        }
    ],
    "minimum-stability": "beta",
    "prefer-stable": true,
    "require": {
        "php": ">=5.4",
        "jane/open-api": "^1.0",
